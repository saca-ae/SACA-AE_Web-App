    <h2>Detalles del Proyecto</h2>
    <hr />
    <div class="span4">
        <h4>
            <span class="label label-info">
                @Html.DisplayNameFor(model => model.Nombre)

                @Html.DisplayFor(model => model.Nombre)
            </span>
        </h4>

        <h4>
            <span class="label label-info">
